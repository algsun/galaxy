<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.microwise.orion.bean">
	<class name="UserZone" table="o_user_zone">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="userId" column="userId" type="java.lang.Integer" />
		<property name="zoneId" column="zoneId" type="java.lang.String" />
	</class>
</hibernate-mapping>
