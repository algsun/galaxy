<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.microwise.orion.bean">
    <class name="InventoryZone" table="o_inventory_zone">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="statisticsType" type="java.lang.Integer" column="statisticsType"/>
        <property name="countNo" type="java.lang.Integer" column="countNo"/>
        <property name="sumNo" type="java.lang.Integer" column="sumNo"/>
        <property name="zoneId" type="java.lang.String" column="zoneId"/>
        <property name="inventoryId" type="java.lang.Integer" column="inventoryId"/>
    </class>
</hibernate-mapping>
